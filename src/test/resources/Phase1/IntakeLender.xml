<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression_IntakeLender" parallel="methods" verbose="1" thread-count="4">  
<!--<suite name="Regression_CC" >-->
	<listeners>
		<listener class-name="bln.util.ExtentReporterNG" />
	</listeners>
	<test name="Regression_CC_Bln">
		<method-selectors>
		<method-selector>
			<script language="beanshell"><![CDATA[groups.containsKey("Regression") && groups.containsKey("IntakeLender")]]>
			</script>
		</method-selector>
	</method-selectors>
		<classes>
				<class name="bln.test.automation.lender.IntakeLender" />
		</classes>
	</test>
</suite>
